{
  "name": "@agencebio/cartobio-front",
  "version": "2.0.0-alpha",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "test": "exit 0",
    "posttest": "eslint ./src"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.6.0",
    "eslint-plugin-vue": "^8.2.0",
    "vite": "^2.7.2",
    "vite-plugin-pages": "^0.19.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-mutating-props": "warn",
      "vue/no-multiple-template-root": "off",
      "vue/script-setup-uses-vars": "error"
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
