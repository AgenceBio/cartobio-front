{
  "name": "app",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "update-stats": "node bin/update-stats",
    "export": "node bin/export",
    "postexport": "npm run postexport:shp && npm run postexport:gpkg",
    "postexport:shp": "cat export.geojson | npx geojson2shp > export.shp.zip",
    "postexport:gpkg": "ogr2ogr -f GPKG -nln cartobio export.gpkg export.geojson",
    "serve": "npm start",
    "build": "vue-cli-service build",
    "test": "npm run lint",
    "lint": "vue-cli-service lint"
  },
  "engines": {
    "node": "12.x.x"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "@turf/angle": "^6.0.1",
    "@turf/line-intersect": "^6.0.2",
    "@turf/polygon-to-line": "^6.0.3",
    "axios": "^0.19.2",
    "deepmerge": "^4.2.2",
    "geojson-vt": "^3.2.1",
    "lodash": "^4.17.19",
    "mapbox-gl": "^1.8.1",
    "turf": "^3.0.14",
    "vue": "^2.6.11",
    "vue-ls": "^3.2.1",
    "vue-mapbox": "^0.4.1",
    "vue-mdc-adapter": "^0.18.2",
    "vue-meta": "^2.3.3",
    "vue-router": "^3.1.6",
    "vuetify": "^1.5.24",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@mapbox/geojson-area": "^0.2.2",
    "@mapbox/polylabel": "^1.0.2",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "JSONStream": "^1.3.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "frontmatter-markdown-loader": "^3.2.1",
    "gdal-next": "^1.0.2",
    "globby": "^11.0.0",
    "listr": "^0.14.3",
    "markdown-it": "^10.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-prism": "^2.0.5",
    "minimist": "^1.2.5",
    "node-sass": "^4.13.1",
    "ora": "^4.0.3",
    "prismjs": "^1.19.0",
    "sass-loader": "^8.0.2",
    "string-replace-loader": "^2.2.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-fullpage": "^1.0.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.4",
    "worker-farm": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
