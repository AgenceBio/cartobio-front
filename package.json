{
  "name": "app",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "serve": "npm start",
    "build": "vue-cli-service build",
    "posttest": "vue-cli-service lint",
    "start": "vue-cli-service serve",
    "test": "vue-cli-service test:unit",
    "update-stats": "node bin/update-stats"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "axios": "^0.19.2",
    "geojson-vt": "^3.2.1",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.8.1",
    "turf": "^3.0.14",
    "vue": "^2.6.11",
    "vue-ls": "^3.2.1",
    "vue-mapbox": "^0.4.1",
    "vue-mdc-adapter": "^0.18.2",
    "vue-meta": "^2.3.3",
    "vue-router": "^3.1.6",
    "vuetify": "^1.5.24",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@mapbox/geojson-area": "^0.2.2",
    "@mapbox/polylabel": "^1.0.2",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.3.0",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "1.0.0-beta.32",
    "JSONStream": "^1.3.5",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "frontmatter-markdown-loader": "^3.2.1",
    "markdown-it": "^10.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-prism": "^2.0.5",
    "node-sass": "^4.13.1",
    "ora": "^4.0.3",
    "prismjs": "^1.19.0",
    "sass-loader": "^8.0.2",
    "string-replace-loader": "^2.2.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-fullpage": "^1.0.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "12.x.x"
  }
}
